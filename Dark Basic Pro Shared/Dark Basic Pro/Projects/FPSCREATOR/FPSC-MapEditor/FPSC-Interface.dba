  wait for file map event 1
  if get file map dword ( 1, 4 ) = 1
   gosub _interface_saveprefs
   leavedialog=1
  endif
  if get file map dword ( 1, 8 ) = 1
   leavedialog=1
  endif
  if get file map dword ( 1, 0 ) = 0
   leavedialog=1
  endif
  close file map 1
  sync
 endwhile
return


